movies) return <div className=\"main-view\"/>;\n\n        return (\n            <div className=\"main-view\">\n             {selectedMovie\n                ? <MovieView movie={selectedMovie}/>\n                : movies.map(movie => (\n                  <MovieCard key={movie._id} movie={movie} onClick={movie => this.onMovieClick(movie)}/>\n                ))\n             }\n            </div>\n           );\n    }\n  }"},"lineCount":null}},"error":null,"hash":"602e91e3167519d61fea1c73b140a57a","cacheData":{"env":{}}}